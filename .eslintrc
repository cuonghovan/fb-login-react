{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "quotmark": "single",
    "react/prop-types": "error",
    "react/no-did-mount-set-state": "error",
    "react/no-did-update-set-state": "error"
  }
}
